{
  "name": "dbml-formatter",
  "displayName": "DBML Formatter - The best and only DBML formatter",
  "description": "Format .dbml files with ease",
  "version": "0.0.4",
  "repository": {
    "type": "git",
    "url": "https://github.com/idoaflalo/dbml-formatter"
  },
  "icon": "assets/icon.png",
  "publisher": "aflalo",
  "engines": {
    "vscode": "^1.80.0"
  },
  "keywords": [
    "dbml",
    "formatter",
    "database",
    "sql",
    "mongodb",
    "mysql",
    "postgres",
    "sqlite",
    "mariadb",
    "db2",
    "oracle",
    "mssql"
  ],
  "categories": [
    "Formatters",
    "Programming Languages",
    "Other"
  ],
  "activationEvents": [
    "onLanguage:dbml"
  ],
  "main": "./extension.js",
  "contributes": {
    "languages": [
      {
        "id": "dbml",
        "aliases": [
          "DBML",
          "dbml"
        ],
        "extensions": [
          ".dbml"
        ]
      }
    ]
  },
  "scripts": {
    "publish": "vsce publish",
    "compile": "vsce package",
    "revert-sample": "git checkout HEAD -- test/sample.dbml"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@vscode/vsce": "^3.2.1"
  },
  "dependencies": {
    "@dbml/core": "^3.9.5"
  }
}
