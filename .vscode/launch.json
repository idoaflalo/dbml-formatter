{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Extension with DBML File",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}",
        "--new-window",
        "${workspaceFolder}/test/sample.dbml"
      ],
      "outFiles": ["${workspaceFolder}/out/**/*.js"],
      "preLaunchTask": "revert-sample"
    }
  ]
}
